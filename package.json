{
  "name": "vue-app",
  "version": "0.0.1",
  "description": "A Vue app",
  "scripts": {
    "start": "npm run clean-dist && npm run template-watch & npm run webpack-dev",
    "build": "npm run clean-dist && npm run template-build && npm run webpack-build",
    "clean-dist": "rm -rf public",
    "template-build": "jade app/templates --out public",
    "template-watch": "fsmonitor -s -d app/templates/ '+*.jade' jade app/templates --out public",
    "webpack-build": "webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "webpack-dev-server --inline --hot --content-base public/ --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "vue": "1.0"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-runtime": "^5.8.0",
    "fsmonitor": "^0.2.4",
    "html-webpack-plugin": "^1.7.0",
    "jade": "1.11.0",
    "karma": "^0.13.16",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-tap": "^1.0.3",
    "karma-tape-reporter": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.19",
    "tape": "^4.4.0",
    "template-html-loader": "0.0.3",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "author": "Danu Widatama",
  "license": "MIT"
}
