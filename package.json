{
  "name": "tic-tac-vue",
  "description": "Two player tic tac toe game implemented with Vue",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest run"
  },
  "author": "Danu Widatama",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widatama/tic-tac-vue.git"
  },
  "type": "module",
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "pug": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-pwa": "^0.18.1",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}
