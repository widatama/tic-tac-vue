{
  "name": "tic-tac-vue",
  "description": "Two player tic tac toe game implemented with Vue",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest run"
  },
  "author": "Danu Widatama",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widatama/tic-tac-vue.git"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "pug": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.0.24"
  }
}
